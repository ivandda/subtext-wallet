generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URI")
}

model Wallet {
  id        String   @id @default(cuid())
  userId    String   @unique
  mnemonic  String
  address   String   @unique
  publicKey String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}